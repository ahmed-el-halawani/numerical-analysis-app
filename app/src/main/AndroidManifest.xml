<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Numerical Interpolation"
        android:roundIcon="@drawable/numerical"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.LagrangeDividedMethod"></activity>
        <activity
            android:name=".ui.MethodActivity"
            android:label="Solution"
            android:parentActivityName=".ui.HomeScreen" />
        <activity
            android:name=".ui.HomeScreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.TableActivity"
            android:label="Differentiation table" />
    </application>

</manifest>